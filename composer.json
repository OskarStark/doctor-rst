{
    "name": "oskarstark/doctor-rst",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "composer/semver": "^3.4.3",
        "ondram/ci-detector": "^4.2.0",
        "symfony/config": "^7.1.1",
        "symfony/console": "^7.1.5",
        "symfony/dependency-injection": "^7.1.5",
        "symfony/finder": "^7.1.4",
        "symfony/options-resolver": "^7.1.1",
        "symfony/service-contracts": "^3.5.0",
        "symfony/string": "^7.1.5",
        "symfony/yaml": "^7.1.5",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.44.0",
        "ergebnis/php-cs-fixer-config": "^6.37",
        "ergebnis/test-util": "^1.6",
        "icanhazstring/composer-unused": "^0.8.11",
        "mikey179/vfsstream": "^1.6.12",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^1.12.6",
        "phpstan/phpstan-phpunit": "^1.4.0",
        "phpstan/phpstan-webmozart-assert": "^1.2.11",
        "phpunit/phpunit": "^10.5.27",
        "rector/rector": "^1.2.5",
        "symfony/var-dumper": "^7.1.5"
    },
    "replace": {
        "paragonie/random_compat": "^2.0",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/doctor-rst"
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
