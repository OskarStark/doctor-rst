{
    "name": "oskarstark/doctor-rst",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "composer/semver": "^3.4.3",
        "ondram/ci-detector": "^4.2.0",
        "symfony/config": "^7.2.3",
        "symfony/console": "^7.2.5",
        "symfony/dependency-injection": "^7.2.5",
        "symfony/finder": "^7.2.2",
        "symfony/options-resolver": "^7.2.0",
        "symfony/service-contracts": "^3.5.1",
        "symfony/string": "^7.2.0",
        "symfony/yaml": "^7.2.5",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.46.0",
        "ergebnis/data-provider": "^3.4",
        "ergebnis/php-cs-fixer-config": "^6.46.0",
        "friendsofphp/php-cs-fixer": "^3.75.0",
        "icanhazstring/composer-unused": "^0.9.2",
        "maglnet/composer-require-checker": "^4.16.1",
        "mikey179/vfsstream": "^1.6.12",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.11",
        "phpstan/phpstan-phpunit": "^2.0.6",
        "phpstan/phpstan-webmozart-assert": "^2.0.0",
        "phpunit/phpunit": "^12.1.2",
        "rector/rector": "^2.0.11",
        "symfony/var-dumper": "^7.2.3"
    },
    "replace": {
        "paragonie/random_compat": "^2.0",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/doctor-rst"
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
